    <h1>MAP</h1>
   <div id="map"></div>

    
    <script>
        let map;

        const myLatLng={ lat: 40.7127281, lng: -74.0060152 };

                function initMap() {
          map = new google.maps.Map(document.getElementById("map"), {
            center: { lat: 40.7127281, lng: -74.0060152 },
            zoom: 15,
          });

         new google.maps.Marker({
    position: myLatLng,
    map,
    title: "Hello World!",
  });

//window.setTimeout(function() {map.setZoom(pos);},3000);
        }


       </script>
<script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyAUa7j3ylZbZ3FHQsEfiSWKBY1CRc5YAEI&callback=initMap&v=weekly"
